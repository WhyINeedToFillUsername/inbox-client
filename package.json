{
  "name": "inbox-client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "browserify modules/inbox.js -o public/javascripts/bundle-inbox.js && browserify modules/solid-login.js -o public/javascripts/bundle-solid-login.js",
    "watch:js:1": "watchify modules/inbox.js -o public/javascripts/bundle-inbox.js",
    "watch:js:2": "watchify modules/solid-login.js -o public/javascripts/bundle-solid-login.js",
    "watch:app": "nodemon ./bin/www",
    "develop": "run-p watch:js:1 watch:js:2 watch:app"
  },
  "dependencies": {
    "browserify": "^16.5.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "hbs": "^4.1.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "rdf-namespaces": "^1.8.0",
    "solid-auth-client": "^2.4.1",
    "tripledoc": "^4.3.4"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "watchify": "^3.11.1"
  }
}
